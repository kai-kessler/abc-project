enum TokenKind
{
    BAD_TOKEN,
    EOI,
    DECIMAL_LITERAL,
    IDENTIFIER,
    PLUS,
    ASTERISK,
    LPAREN,
    RPAREN,
};

type TokenVal: array[12] of char;

struct Token
{
    kind:   TokenKind;
    pos:    struct Pos {
                row, col: unsigned;
            };
    val:    TokenVal;
};

extern token: Token;

extern fn getToken(): TokenKind;

extern fn tokenKindStr(token: TokenKind): -> char;
